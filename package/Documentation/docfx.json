{
    "metadata": [
       {
           "src": [
               {
                   "src" : "temp",
                   "files": [
                       "**.csproj"
                   ]
               }
           ],
           "dest": "Api"
       }
    ],
    "build": {
       "content": [
         {
           "files": [
             "toc.yml",
             "index.md"
           ]
         },
         {
           "files": [
             "api/toc.yml",
             "api/**/*.yml",
             "api/**/*.md"
           ]
         },
         {
           "files": [
             "manual/**/*.md",
             "manual/**/*.yml"
           ]
         }
       ],
       "resource": [
           {
               "files": [
                   "Media/**"
               ]
           }
       ],
       "overwrite": "",
       "dest": "",
      "globalMetadataFiles": ["metadata.json"],
       "template": [
         "default", 
         "modern"
       ],
      "postProcessors": [],
      "keepFileLink": false,
      "disableGitFeatures": false
    }
}
